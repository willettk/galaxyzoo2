	Author = {{Nelson}, E.~J. and {van Dokkum}, P.~G. and {Brammer}, G. and {F{\"o}rster Schreiber}, N. and {Franx}, M. and {Fumagalli}, M. and {Patel}, S. and {Rix}, H.-W. and {Skelton}, R.~E. and {Bezanson}, R. and {Da Cunha}, E. and {Kriek}, M. and {Labbe}, I. and {Lundgren}, B. and {Quadri}, R. and {Schmidt}, K.~B.},
	Date-Added = {2012-10-12 21:27:26 +0000},
