	Date-Added = {2012-06-26 10:40:36 -0500},
	Date-Modified = {2013-02-22 21:47:34 +0000},
	Doi = {10.1111/j.1365-2966.2012.21146.x},
	Eprint = {1110.6320},
	Journal = {\mnras},
	Keywords = {galaxies: abundances, galaxies: clusters: general, galaxies: groups: general},
	Month = jun,
	Pages = {3144},
