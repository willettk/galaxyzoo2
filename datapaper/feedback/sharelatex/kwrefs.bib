	Eprint = {1205.5905},
	Journal = {\mnras},
	Keywords = {methods: numerical, methods: statistical, galaxies: active, galaxies: evolution, galaxies: jets, galaxies: luminosity function, mass function},
	Month = aug,
	Pages = {2028-2054},
	Primaryclass = {astro-ph.CO},
	Title = {{Fundamental properties of Fanaroff-Riley type II radio galaxies investigated via Monte Carlo simulations}},
