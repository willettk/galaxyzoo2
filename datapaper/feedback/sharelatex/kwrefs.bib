	Journal = {Physics Letters B},
	Month = aug,
	Pages = {25-29},
	Primaryclass = {astro-ph.CO},
	Title = {{Handedness asymmetry of spiral galaxies with z{\lt}0.3 shows cosmic parity violation and a dipole axis}},
