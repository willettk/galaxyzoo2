	Doi = {10.1088/0004-637X/753/1/7},
	Eid = {7},
	Eprint = {1205.6863},
	Journal = {\apj},
	Keywords = {galaxies: individual: M31, galaxies: kinematics and dynamics, Local Group, proper motions},
	Month = jul,
	Pages = {7},
