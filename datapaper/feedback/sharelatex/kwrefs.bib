	Eprint = {1207.4196},
	Journal = {\nat},
	Month = jul,
	Pages = {338-340},
	Primaryclass = {astro-ph.CO},
	Title = {{High velocity dispersion in a rare grand-design spiral galaxy at redshift z = 2.18}},
