	Doi = {10.1088/0004-637X/755/2/105},
	Eid = {105},
	Eprint = {1206.5805},
	Journal = {\apj},
	Keywords = {galaxies: elliptical and lenticular, cD, galaxies: evolution, ultraviolet: galaxies},
	Month = aug,
	Pages = {105},
	Primaryclass = {astro-ph.CO},
