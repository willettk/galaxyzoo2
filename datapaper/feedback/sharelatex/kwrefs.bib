	Doi = {10.1088/0004-637X/692/1/L19},
	Eprint = {0901.1663},
	Journal = {\apjl},
	Keywords = {galaxies: active, galaxies: evolution, galaxies: formation, galaxies: nuclei, X-rays: galaxies},
	Month = feb,
	Pages = {L19-L23},
