	Doi = {10.1086/318944},
	Journal = {\apjs},
	Keywords = {Galaxies: Active, Galaxies: Fundamental Parameters, Galaxies: ISM, Galaxies: Starburst, Infrared: Galaxies, Techniques: Spectroscopic},
	Month = jan,
	Pages = {37-71},
	Title = {{Optical Classification of Southern Warm Infrared Galaxies}},
	Volume = 132,
