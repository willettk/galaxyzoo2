	Doi = {10.1088/0004-637X/748/1/68},
	Eid = {68},
	Eprint = {1203.0568},
	Journal = {\apj},
	Keywords = {BL Lacertae objects: general, gamma rays: galaxies, infrared: galaxies, radiation mechanisms: non-thermal},
	Month = mar,
	Pages = {68},
