	Doi = {10.1088/0004-637X/751/1/52},
	Eid = {52},
	Eprint = {1203.1942},
	Journal = {\apj},
	Keywords = {galaxies: active, galaxies: Seyfert, methods: statistical, quasars: general, surveys},
	Month = may,
	Pages = {52},
	Primaryclass = {astro-ph.CO},
