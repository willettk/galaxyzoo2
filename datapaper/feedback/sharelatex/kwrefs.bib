	Journal = {\mnras},
	Keywords = {GALAXIES: ACTIVE, GALAXIES: CLUSTERS: GENERAL, GALAXIES: LUMINOSITY FUNCTION, MASS FUNCTION, RADIO CONTINUUM: GALAXIES},
	Month = sep,
	Pages = {720-736},
	Title = {{The cluster environments of the z\~{}1 3CR radio galaxies}},
