	Keywords = {galaxies: photometry, infrared: galaxies, quasars: absorption lines},
	Month = oct,
	Pages = {124},
	Primaryclass = {astro-ph.CO},
	Title = {{Invisible Active Galactic Nuclei. I. Sample Selection and Optical/Near-IR Spectral Energy Distributions}},
	Volume = 144,
	Year = 2012,
