	Eprint = {arXiv:astro-ph/9809060},
	Journal = {\apj},
	Keywords = {GALAXIES: BL LACERTAE OBJECTS: GENERAL, GALAXIES: ELLIPTICAL AND LENTICULAR, CD, GALAXIES: STRUCTURE},
	Month = feb,
	Pages = {88-99},
