	Keywords = {surveys: galaxies: distances and redshifts: galaxies: luminosity function, mass function: cosmology: observations: large-scale structure of Universe, surveys, galaxies: distances and redshifts, galaxies: luminosity function, mass function, cosmology: observations, large-scale structure of Universe},
