	Journal = {\mnras},
	Keywords = {methods: statistical, galaxies: evolution, galaxies: fundamental parameters, galaxies: general, galaxies: haloes, galaxies: statistics},
	Month = jan,
	Pages = {2133-2146},
	Primaryclass = {astro-ph.CO},
	Title = {{Disentangling galaxy environment and host halo mass}},
