	Month = feb,
	Pages = {1355-1366},
	Primaryclass = {astro-ph.EP},
	Title = {{The distribution of interplanetary dust between 0.96 and 1.04 au as inferred from impacts on the STEREO spacecraft observed by the heliospheric imagers}},
