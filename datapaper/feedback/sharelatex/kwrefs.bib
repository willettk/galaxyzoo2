	Date-Modified = {2013-02-22 21:47:34 +0000},
	Doi = {10.1002/asna.201211705},
	Eprint = {1205.4747},
	Journal = {Astronomische Nachrichten},
	Keywords = {cosmology: large-scale structure, ISM: clouds, methods: data analysis, techniques: image processing, techniques: radial velocities},
	Pages = {505},
	Primaryclass = {astro-ph.IM},
	Title = {{Principles of high-dimensional data visualization in astronomy}},
