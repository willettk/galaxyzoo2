	Archiveprefix = {arXiv},
	Author = {{Hambleton}, K.~M. and {Gibson}, B.~K. and {Brook}, C.~B. and {Stinson}, G.~S. and {Conselice}, C.~J. and {Bailin}, J. and {Couchman}, H. and {Wadsley}, J.},
