	Journal = {\aj},
	Keywords = {galaxies: bulges, galaxies: fundamental parameters, galaxies: structure, techniques: image processing, techniques: photometric},
	Month = jun,
	Pages = {2097-2129},
	Primaryclass = {astro-ph.CO},
	Title = {{Detailed Decomposition of Galaxy Images. II. Beyond Axisymmetric Models}},
