	Journal = {\apj},
	Keywords = {Galaxy: center, ISM: clouds, planets and satellites: formation, stars: kinematics and dynamics, stars: winds, outflows},
	Month = sep,
	Pages = {86},
	Primaryclass = {astro-ph.GA},
	Title = {{A Star Disrupted by a Stellar Black Hole as the Origin of the Cloud Falling toward the Galactic Center}},
	Volume = 756,
