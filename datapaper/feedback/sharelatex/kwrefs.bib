	Date-Added = {2012-11-13 16:45:53 +0000},
	Date-Modified = {2013-02-22 21:47:35 +0000},
	Eprint = {1211.2023},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Galaxy Astrophysics, Astrophysics - Cosmology and Extragalactic Astrophysics},
	Month = nov,
	Primaryclass = {astro-ph.GA},
	Title = {{An Arecibo Survey for Zeeman Splitting in OH Megamaser Galaxies}},
	Year = 2012,
	Bdsk-File-1 = {}}

@article{men12,
	Abstract = {The effects that interactions produce on galaxy disks and how they modify the subsequent formation of bars need to be distinguished to fully understand the relationship between bars and environment. To this aim we derive the bar fraction in three different environments ranging from the field to Virgo and Coma clusters, covering an unprecedentedly large range of galaxy luminosities (or, equivalently, stellar masses). We confirm that the fraction of barred galaxies strongly depends on galaxy luminosity. We also show that the difference between the bar fraction distributions as a function of galaxy luminosity (and mass) in the field and Coma cluster are statistically significant, with Virgo being an intermediate case. The fraction of barred galaxies shows a maximum of about 50% at \,\simeq\,-20.5$ in clusters, whereas the peak is shifted to \,\simeq\,-19$ in the field. We interpret this result as a variation of the effect of environment on bar formation depending on galaxy luminosity. We speculate that brighter disk galaxies are stable enough against interactions to keep their cold structure, thus, the interactions are able to trigger bar formation. For fainter galaxies the interactions become strong enough to heat up the disks inhibiting bar formation and even destroying the disks. Finally, we point out that the controversy regarding whether the bar fraction depends on environment could be resolved by taking into account the different luminosity ranges probed by the galaxy samples studied so far.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2012arXiv1211.1007M},
	Archiveprefix = {arXiv},
	Author = {{Mendez-Abreu}, J. and {Sanchez-Janssen}, R. and {Aguerri}, J.~A.~L. and {Corsini}, E.~M. and {Zarattini}, S.},
	Date-Added = {2012-11-07 16:59:21 +0000},
	Date-Modified = {2013-02-22 21:47:35 +0000},
	Eprint = {1211.1007},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
	Month = nov,
	Primaryclass = {astro-ph.CO},
	Title = {{The nature and nurture of bars and disks}},
	Year = 2012,
