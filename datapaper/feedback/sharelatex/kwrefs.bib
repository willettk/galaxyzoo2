	Doi = {10.1111/j.1365-2966.2012.21554.x},
	Eprint = {1205.0808},
	Journal = {\mnras},
	Keywords = {radiation mechanisms: non-thermal, galaxies: active, galaxies: jets},
	Month = sep,
	Pages = {1371-1379},
