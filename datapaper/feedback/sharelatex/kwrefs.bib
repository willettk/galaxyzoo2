	Keywords = {Accretion, Accretion Disks, Galaxies: Active, Galaxies: Jets, Galaxies: Nuclei, Radio Continuum: Galaxies, Surveys},
	Month = oct,
	Pages = {L87-L90},
	Title = {{Evidence for Jet Domination of the Nuclear Radio Emission in Low-Luminosity Active Galactic Nuclei}},
	Volume = 559,
	Year = 2001,
