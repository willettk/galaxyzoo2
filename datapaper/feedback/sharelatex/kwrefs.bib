	Adsurl = {http://adsabs.harvard.edu/abs/2010MNRAS.404..792M},
	Archiveprefix = {arXiv},
	Author = {{Masters}, K.~L. and {Nichol}, R. and {Bamford}, S. and {Mosleh}, M. and {Lintott}, C.~J. and {Andreescu}, D. and {Edmondson}, E.~M. and {Keel}, W.~C. and {Murray}, P. and {Raddick}, M.~J. and {Schawinski}, K. and {Slosar}, A. and {Szalay}, A.~S. and {Thomas}, D. and {Vandenberg}, J.},
	Date-Added = {2012-11-29 20:36:49 +0000},
