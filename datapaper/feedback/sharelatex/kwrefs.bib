	Keywords = {Cosmology: Dark Matter, Galaxies: Clusters: General, Galaxies: Fundamental Parameters, Galaxies: Photometry, Cosmology: Large-Scale Structure of Universe},
	Month = jul,
	Pages = {150-163},
	Title = {{Near-Infrared Properties of Moderate-Redshift Galaxy Clusters. II. Halo Occupation Number, Mass-to-Light Ratios, and {$\Omega$}$_{m}$}},
