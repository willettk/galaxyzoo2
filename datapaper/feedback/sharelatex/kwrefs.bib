	Date-Added = {2012-12-13 17:15:55 +0000},
	Date-Modified = {2013-02-22 21:47:36 +0000},
	Eprint = {1212.2632},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Cosmology and Extragalactic Astrophysics},
	Month = dec,
	Primaryclass = {astro-ph.HE},
	Title = {{Synchrotron and inverse-Compton emission from blazar jets - II. An accelerating jet model with a geometry set by observations of M87}},
	Year = 2012,
