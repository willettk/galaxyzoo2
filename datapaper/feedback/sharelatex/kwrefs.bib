	Date-Added = {2012-04-12 12:06:28 -0500},
	Date-Modified = {2013-02-22 21:47:35 +0000},
	Doi = {10.1111/j.1365-2966.2010.17834.x},
	Eprint = {1003.0449},
	Journal = {\mnras},
	Keywords = {surveys, galaxies: bulges, galaxies: evolution, galaxies: photometry, galaxies: spiral, galaxies: structure},
