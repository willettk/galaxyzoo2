	Doi = {10.1038/nature10652},
	Eprint = {1112.3264},
	Journal = {\nat},
	Month = jan,
	Pages = {51-54},
	Primaryclass = {astro-ph.GA},
	Title = {{A gas cloud on its way towards the supermassive black hole at the Galactic Centre}},
