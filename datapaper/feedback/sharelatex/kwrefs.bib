	Adsurl = {http://adsabs.harvard.edu/abs/2009ApJ...698..594M},
	Archiveprefix = {arXiv},
	Author = {{McNamara}, B.~R. and {Kazemzadeh}, F. and {Rafferty}, D.~A. and {B{\^\i}rzan}, L. and {Nulsen}, P.~E.~J. and {Kirkpatrick}, C.~C. and {Wise}, M.~W.},
	Date-Added = {2012-03-12 11:51:03 -0500},
