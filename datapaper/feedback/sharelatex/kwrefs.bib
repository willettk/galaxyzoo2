	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Galaxy Astrophysics},
	Month = jul,
	Primaryclass = {astro-ph.GA},
	Title = {{Identifying Local Group Field Galaxies which have interacted with the Milky Way}},
	Year = 2012,
