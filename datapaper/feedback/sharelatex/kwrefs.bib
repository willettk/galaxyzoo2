	Keywords = {cosmic rays, galaxies: active, gamma rays: galaxies, gamma rays: general, gamma rays: ISM},
	Month = feb,
	Pages = {196},
	Primaryclass = {astro-ph.HE},
	Title = {{Lower Limits on Ultrahigh-energy Cosmic Ray and Jet powers of TeV Blazars}},
