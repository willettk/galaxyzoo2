	Keywords = {galaxies: active, galaxies: bulges, galaxies: fundamental parameters, galaxies: nuclei, galaxies: Seyfert},
	Month = dec,
	Pages = {75},
	Primaryclass = {astro-ph.CO},
	Title = {{Moderate-luminosity Growing Black Holes from 1.25 {\lt} z {\lt} 2.7: Varied Accretion in Disk-dominated Hosts}},
