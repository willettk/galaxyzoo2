	Eprint = {arXiv:astro-ph/9902249},
	Journal = {\apj},
	Keywords = {COSMOLOGY: OBSERVATIONS, GALAXIES: EVOLUTION, GALAXIES: FUNDAMENTAL PARAMETERS, GALAXIES: LUMINOSITY FUNCTION, MASS FUNCTION, SURVEYS},
	Month = jun,
