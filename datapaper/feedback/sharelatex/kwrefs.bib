	Eprint = {1204.0790},
	Journal = {\apj},
	Keywords = {galaxies: active, gamma rays: galaxies, methods: statistical, radio continuum: galaxies},
	Month = jun,
	Pages = {149},
	Primaryclass = {astro-ph.HE},
	Title = {{Assessing the Significance of Apparent Correlations between Radio and Gamma-Ray Blazar Fluxes}},
	Volume = 751,
