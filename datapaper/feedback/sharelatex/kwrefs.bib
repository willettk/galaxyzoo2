	Author = {{D'Abrusco}, R. and {Massaro}, F. and {Ajello}, M. and {Grindlay}, J.~E. and {Smith}, H.~A. and {Tosti}, G.},
	Date-Added = {2012-09-11 13:23:44 -0700},
