	Archiveprefix = {arXiv},
	Author = {{Richards}, J.~W. and {Starr}, D.~L. and {Brink}, H. and {Miller}, A.~A. and {Bloom}, J.~S. and {Butler}, N.~R. and {Berian James}, J. and {Long}, J.~P. and {Rice}, J.},
	Date-Added = {2011-06-16 11:36:34 -0600},
