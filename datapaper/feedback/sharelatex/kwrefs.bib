	Month = jul,
	Pages = {666},
	Series = {Astronomical Society of the Pacific Conference Series},
	Title = {{ImgCutout, an Engine of Instantaneous Astronomical Discovery}},
	Volume = 314,
	Year = 2004,
	Bdsk-File-1 = {}}

@article{zha07,
	Abstract = {We use a large sample of 174 Wolf-Rayet (W-R) galaxies drawn from the Sloan Digital Sky Survey to study whether and how the slope of the stellar initial mass function depends on metallicity. We calculate for each object its oxygen abundance, according to which we divide our sample into four metallicity subsamples. For each subsample, we then measure three quantities: the equivalent width of the Hbeta emission line, the equivalent width of the W-R bump around 4650 {\AA}, and the W-R bump-to-Hbeta intensity ratio, and compare to the predictions of the same quantities by evolutionary synthesis models of Schaerer & Vacca. Such comparisons lead to a clear dependence of the slope of initial mass function (alpha) on metallicity in that galaxies at higher metallicities tend to have steeper initial mass functions, with the slope index ranging from alpha~1.00 for the lowest metallicity, Z=0.001, to alpha~3.30 for the highest metallicity, Z=0.02. We have carefully examined the possible sources of systematic error, either in models or in our observational measurements, and have shown that these sources do not change this result.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2007ApJ...655..851Z},
	Author = {{Zhang}, W. and {Kong}, X. and {Li}, C. and {Zhou}, H.-Y. and {Cheng}, F.-Z.},
	Date-Added = {2013-02-05 20:44:15 +0000},
