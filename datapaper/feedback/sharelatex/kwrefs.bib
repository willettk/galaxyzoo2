@article{gre84,
	Abstract = {Images in Gunn r (6500 + or - 450 A) of fields around 108 bright quasars have been obtained using the SIT-vidicon camera on the Palomar 1.52 m telescope for the purpose of studying the global environments of quasars. The sample consists of quasars from complete optical and radio surveys, with redshifts ranging from 0.05 to over 2.0. A catalog of the positional offsets from the quasar, the magnitudes, and the classifications (into stars or galaxies) of the detected objects is presented. In addition, control fields 1 deg N of each quasar, for the purpose of background galaxy counting, were also observed. The background galaxy counts derived from the control fields, covering a total area of 0.226 sq deg, are in good agreement with the counts obtained by Kron and Sebok.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
