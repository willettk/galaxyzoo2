	Archiveprefix = {arXiv},
	Author = {{Hoyle}, B. and {Masters}, K.~L. and {Nichol}, R.~C. and {Edmondson}, E.~M. and {Smith}, A.~M. and {Lintott}, C. and {Scranton}, R. and {Bamford}, S. and {Schawinski}, K. and {Thomas}, D.},
	Date-Added = {2012-06-26 10:40:33 -0500},
	Date-Modified = {2013-02-22 21:47:34 +0000},
